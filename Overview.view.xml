<mvc:View
	controllerName="sap.ui.demo.wt.App"
	xmlns="sap.m"
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>homePageTitle}">
		<content>
		<TileContainer
			id="container"
			tiles="{view>/Employees}">
			<StandardTile
				number="{view>EmployeeID}"
				title="{view>FirstName}"
				info="{view>Title}"
				type="Active"
				press="onPress" />
		</TileContainer>
		</content>
	</Page>
</mvc:View>